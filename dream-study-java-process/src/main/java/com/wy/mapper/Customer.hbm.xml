<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.itheimaoa.domain.customer.Customer">
		<id name="id" length="36">
			<generator class="uuid"></generator>
		</id>
		<property name="name" length="20" not-null="true"></property>
		<property name="gender" length="5" ></property>
		<property name="cellphone" length="15" ></property>
		<property name="qq" length="15" ></property>
		<property name="email" length="30" ></property>
		<property name="address" length="100" ></property>
		<property name="customerStatus" length="20" ></property>
		<property name="infoSource" length="20" ></property>
		<property name="intentionCourse" length="20" ></property>
		<property name="message" length="200" ></property>
	</class>
</hibernate-mapping>